   "source": [
    "def A(m, n):\n",
    "    if m == 0:\n",
    "        return 2 * n\n",
    "    elif m >= 1 and n == 0:\n",
    "        return 0\n",
    "    elif m >= 1 and n == 1:\n",
    "        return 2\n",
    "    else:\n",
    "        return A(m - 1, A(m, n - 1))"
   ]
